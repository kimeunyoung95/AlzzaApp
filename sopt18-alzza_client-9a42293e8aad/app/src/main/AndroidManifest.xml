<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.sopt.alzza">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:name=".application.App"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppThemeNoActionBar">

        <activity android:name=".activity.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.LoginActivity"/>
        <activity android:name=".activity.JoinActivity"/>
        <activity android:name=".activity.LectureActivity"/>
        <activity android:name=".activity.MainActivity"/>
        <activity android:name=".activity.ChatActivity"/>
        <activity android:name=".activity.ChatLoginActivity"/>
        <activity android:name=".activity.SelectSubjectActivity"/>
        <activity android:name=".activity.SelectAppActivity"/>
        <activity android:name=".activity.SelectDetailActivity"/>


    </application>

</manifest>